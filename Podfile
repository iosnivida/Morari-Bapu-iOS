  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  pod 'IQKeyboardManagerSwift', ' ~> 6.0.3'
  
	# For Using Called Web Service
	pod 'Alamofire', '~> 4.7.2'
	pod 'SwiftyJSON', '~> 4.1.0'
    
  pod 'SVProgressHUD'
  pod 'Kingfisher', '~> 4.7.0'
  
  pod 'Toast-Swift','~> 3.0.1'

  # UserDefault Manage's
  pod 'SwiftyUserDefaults', '~> 3.0.1'

  
  pod 'ESTMusicIndicator'

  # Pods for Doggie

  pod 'iCarousel', '~> 1.8'
  
  # Audio Player
  pod 'Jukebox'

  pod 'FolioReaderKit'
  
  # Pods for Morari Bapu

  target 'Morari BapuTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Morari BapuUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
